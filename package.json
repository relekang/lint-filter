{
  "name": "lint-filter",
  "version": "1.7.3",
  "description": "Lint touched code stricter",
  "main": "./lib",
  "bin": "./lint-filter.js",
  "scripts": {
    "prepublish": "babel src -d lib",
    "test": "nyc -a -r @babel/register ava",
    "lint": "eslint --cache  .",
    "flow": "flow check",
    "coverage": "nyc -a --reporter cobertura ava --verbose",
    "integration-test": "bash integration-test",
    "lint:errors": "eslint --cache --quiet  .",
    "format": "eslint --cache --quiet --fix  ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/relekang/lint-filter.git"
  },
  "keywords": [
    "lint"
  ],
  "author": "Rolf Erik Lekang <me@rolflekang.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/relekang/lint-filter/issues"
  },
  "homepage": "https://github.com/relekang/lint-filter#readme",
  "ava": {
    "files": [
      "test/formatters/*.js",
      "test/utils/*.js",
      "test/*.js"
    ],
    "babel": true,
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "nyc": {
    "include": [
      "src/**"
    ],
    "exclude": [
      "flow-typed/**",
      "test/**"
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "chalk": "^1.1.3",
    "checkstyle-formatter": "^1.1.0",
    "commander": "^2.9.0",
    "get-stdin": "^5.0.1",
    "in-range": "^1.0.0",
    "lodash": "^4.17.14",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/eslint-parser": "^7.16.5",
    "@babel/node": "^7.16.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/register": "^7.16.9",
    "@rushstack/eslint-patch": "^1.1.0",
    "ava": "^3.15.0",
    "eslint": "^8.8.0",
    "eslint-config-relekang": "^2.1.10",
    "flow-bin": "^0.32.0",
    "nyc": "^15.1.0",
    "sinon": "^1.17.7",
    "standard-version": "^9.3.2"
  }
}
